import unittest

class TestCase(unittest.TestCase):
  def test1(self):
    self.assertEqual(sum(2, 1),3)
  def test2(self):
    self.assertEqual(sum(-1, 2), 1)
  def test3(self):
    self.assertEqual(sum(-1, -1),-2)
  def test4(self):
    self.assertEqual(sum(1.5, 5), 6.5)

side = input("What would you like the length of the side of your cube to be: ")
volume = side*side*side
print("The volume of your cube is " volume)

if __name__ == '__main__':
unittest.main(verbosity=2)
